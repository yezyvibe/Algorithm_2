import heapq


def solution(stock, dates, supplies, k):
    answer = 0
    possible_supplies = []
    idx = 0

    while stock < k:
        for i in range(idx, len(dates)):
            if dates[i] <= stock:
                heapq.heappush(possible_supplies, (-supplies[i], supplies[i]))
            else:
                idx = i
                break
        else:
            idx = i + 1
        stock += heapq.heappop(possible_supplies)[1]
        answer += 1

    return answer

